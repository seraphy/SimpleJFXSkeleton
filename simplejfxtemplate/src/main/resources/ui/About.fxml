<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.web.WebView?>


<VBox maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
	prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121"
	xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<WebView fx:id="webview" prefHeight="200.0" prefWidth="200.0"
			VBox.vgrow="ALWAYS" />
		<ButtonBar prefHeight="40.0" prefWidth="200.0">
			<buttons>
				<Button mnemonicParsing="false" onAction="#onShowSysProps"
					text="%sysprops.title" ButtonBar.buttonData="LEFT"/>
				<Button cancelButton="true" mnemonicParsing="false"
					onAction="#onClose" text="%close" ButtonBar.buttonData="CANCEL_CLOSE"/>
			</buttons>
			<VBox.margin>
				<Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
			</VBox.margin>
		</ButtonBar>
	</children>
</VBox>
